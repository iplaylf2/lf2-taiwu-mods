<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <PackageId>LF2.UniTask</PackageId>
        <IncludeBuildOutput>false</IncludeBuildOutput>
        <TargetFramework>$(LF2FrontendTargetFramework)</TargetFramework>
        <Version>2.5.10-2022.3.14f</Version>
        <PackageOutputPath>$(LF2NupkgDir)</PackageOutputPath>
    </PropertyGroup>

    <ItemGroup>
        <Content Include="lib/**/*.*" Pack="true" PackagePath="lib/$(TargetFramework)/" />
    </ItemGroup>
</Project>